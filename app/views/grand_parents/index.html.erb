<h1>Listing grand_parents</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Age</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @grand_parents.each do |grand_parent| %>
  <tr>
    <td><%= grand_parent.name %></td>
    <td><%= grand_parent.age %></td>
    <td><%= link_to 'Show', grand_parent %></td>
    <td><%= link_to 'Edit', edit_grand_parent_path(grand_parent) %></td>
    <td><%= link_to 'Destroy', grand_parent, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Grand parent', new_grand_parent_path %>
